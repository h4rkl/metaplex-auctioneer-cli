{
  "name": "metaplex-auctioneer-cli",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "lint": "prettier -c '{src,test}/**/*.{ts,tsx}' && npm run lint:eslint",
    "lint:check": "eslint \"src/**/*.ts\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "lint:eslint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "lint:fix": "prettier --write '{src,test}/**/*.{ts,tsx}' && eslint --fix '{src,test}/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "pkg": {
    "scripts": "./build/**/*.{js|json}"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.170.0",
    "@bundlr-network/client": "^0.8.5",
    "@metaplex-foundation/mpl-auction-house": "^2.3.0",
    "@metaplex-foundation/mpl-auctioneer": "^0.1.0",
    "@metaplex-foundation/mpl-token-metadata": "1.2.5",
    "@project-serum/anchor": "^0.17.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "1.61.1",
    "@supercharge/promise-pool": "2.3.2",
    "borsh": "^0.7.0",
    "buffer": "^6.0.3",
    "cli-progress": "^3.11.2",
    "commander": "^9.4.0",
    "form-data": "^4.0.0",
    "ipfs-http-client": "^58.0.0",
    "jsonschema": "^1.4.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "mime": "^3.0.0",
    "node-fetch": "^3.2.10",
    "weighted": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.18",
    "@types/webgl2": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
